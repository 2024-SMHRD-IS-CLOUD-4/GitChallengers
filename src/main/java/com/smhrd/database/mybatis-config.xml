<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="com/smhrd/database/db.properties"></properties>
	<typeAliases>
		<typeAlias type="com.smhrd.model.Member" alias = "Member"/>
		<typeAlias type="com.smhrd.model.Group" alias = "Group"/>
		<typeAlias type="com.smhrd.model.Join" alias = "Join"/>
		<typeAlias type="com.smhrd.model.Gc_items" alias = "items"/>
		<typeAlias type="com.smhrd.model.Member_point" alias = "point"/>
		<typeAlias type="com.smhrd.model.Pc_challenge" alias = "Pc_challenge"/>
		<typeAlias type="com.smhrd.model.Pc_items" alias = "Pc_items"/>
		<typeAlias type="com.smhrd.model.Member_info" alias = "info"/>
		<typeAlias type="com.smhrd.model.Review" alias = "Review"/>
		<typeAlias type="com.smhrd.model.Comment" alias = "Comment"/>
		<typeAlias type="com.smhrd.model.Follow" alias = "Follow"/>
		<typeAlias type="com.smhrd.model.Gc_comment" alias = "Gc_comment"/>
		<typeAlias type="com.smhrd.model.Gc_heart" alias = "Heart"/>
		<typeAlias type="com.smhrd.model.Review_heart" alias = "Review_heart"/>
		<typeAlias type="com.smhrd.model.Warning" alias = "Warning"/>
	</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/smhrd/database/MemberMapper.xml"/>
    <mapper resource="com/smhrd/database/GroupMapper.xml"/>
    <mapper resource="com/smhrd/database/JoinMapper.xml"/>
    <mapper resource="com/smhrd/database/Gc_itemsMapper.xml"/>
    <mapper resource="com/smhrd/database/Member_pointMapper.xml"/>
    <mapper resource="com/smhrd/database/Pc_challengeMapper.xml"/>
    <mapper resource="com/smhrd/database/Pc_itemsMapper.xml"/>
    <mapper resource="com/smhrd/database/Member_infoMapper.xml"/>
    <mapper resource="com/smhrd/database/ReviewMapper.xml"/>
    <mapper resource="com/smhrd/database/CommentMapper.xml"/>
    <mapper resource="com/smhrd/database/FollowMapper.xml"/>
    <mapper resource="com/smhrd/database/Gc_commentMapper.xml"/>
    <mapper resource="com/smhrd/database/Gc_heartMapper.xml"/>
    <mapper resource="com/smhrd/database/Review_heartMapper.xml"/>
    <mapper resource="com/smhrd/database/WarningMapper.xml"/>
  </mappers>
</configuration>